   -- The function that takes place when the button is pressed
local defaultWalkSpeed = 16
local boostedWalkSpeed = 45 -- New speed when button is clicked

-- Function to boost speed
local function boostSpeed()
    player.Character.Humanoid.WalkSpeed = boostedWalkSpeed
    wait(9999999) -- Duration for the speed boost
    player.Character.Humanoid.WalkSpeed = defaultWalkSpeed -- Reset speed
end
   end,
})

